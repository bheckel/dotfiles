#articlelist-format  "%4i %f %D   %?T?|%-17T|  ?%t"
articlelist-format  "%3i %?T?|%-17T|  ?%t"
feedlist-format "%5i %n %11u %t"

bind-key j down
bind-key k up
bind-key r random-unread
bind-key R reload

browser w3m

color info green color238
color listfocus yellow color238
color listfocus_unread yellow color238

goto-next-feed yes
show-read-articles no
